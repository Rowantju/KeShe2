    	</Grid>
    	<Button x:ConnectionId='2' x:Name="BackStrongbox1Button" Content="" HorizontalAlignment="Left" Height="63" Margin="190,700,0,0" VerticalAlignment="Top" Width="177" Opacity="0" BorderThickness="0"                                   />
    	<Image x:Name="CoverKeyImage" HorizontalAlignment="Left" Height="66" Margin="523,517,0,0" VerticalAlignment="Top" Width="196" Source="Assets/CoverKeyImage.PNG" Stretch="UniformToFill"/>

    </Grid>
